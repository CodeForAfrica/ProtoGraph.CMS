<%= simple_form_for(@template_stream_card) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :account_id %>
    <%= f.input :template_card_id %>
    <%= f.input :template_stream_id %>
    <%= f.input :is_mandatory %>
    <%= f.input :position %>
    <%= f.input :created_by %>
    <%= f.input :updated_by %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: "ui button" %>
  </div>
<% end %>