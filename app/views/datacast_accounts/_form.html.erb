<%= simple_form_for(@datacast_account) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :datacast_id %>
    <%= f.input :account_id %>
    <%= f.input :is_active %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, class: "ui button" %>
  </div>
<% end %>