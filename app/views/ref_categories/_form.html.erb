<%= simple_form_for(@ref_category) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :site_id %>
    <%= f.input :category %>
    <%= f.input :name %>
    <%= f.input :is_disabled %>
    <%= f.input :created_by %>
    <%= f.input :updated_by %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
